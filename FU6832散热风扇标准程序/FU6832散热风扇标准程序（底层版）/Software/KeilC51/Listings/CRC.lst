  63          uint8 SW_CRC8_Calc(uint8 ptr[], uint8 len)
  64          {
  65   1        uint8 crc = 0x00;
  66   1        uint8 cnt = 0;
  67   1        while (len--)
  68   1        {
  69   2          crc = crc8_Table[crc ^ ptr[cnt++]];
  70   2        }
  71   1        return (crc);
  72   1      }
